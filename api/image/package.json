		"@types/strict-uri-encode": "^2.0.0",
		"prettier": "^2.8.4",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@directus/errors": "^0.0.1",
		"@directus/sdk": "^10.3.1",
		"amqp-connection-manager": "^4.1.12",
		"amqplib": "^0.10.3",
		"nanoid": "^4.0.1",
